              ],
              "default-value": "Pie chart"
            }
          }
        },
        "knn": {
          "name": "k-nearest neighbours (kNN)",
          "description": "",
          "parameters": {
            "number-of-neighbours": {
              "name": "Number of neighbours:",
              "type": "numeric",
              "min": 1,
              "max": 100,
              "step": 1,
              "default-value": 5
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Box plot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
