{
  "analysis-level": "protein",
  "sections": {
    "importing": {
      "main-data-import": {
        "ms-data-import": {
          "name": "MS Data Import",
          "description": "Import MS data",
          "parameters": {
            "file_format": {
              "name": "File format",
              "type": "categorical",
              "categories": [
                "MaxQuant",
                "MSFragger"
              ],
              "default-value": "MaxQuant"
            },
            "intensity_param": {
              "name": "intensity parameter",
              "type": "categorical",
              "categories": [
                "LFQ intensity",
                "Intensity",
                "iBAQ"
              ],
              "default-value": "Intensity"
            }
          }
        }
      },
      "metadata-import": {
        "metadata-import-method": {
          "name": "Metadata Import",
          "description": "Import metadata",
          "parameters": {
            "feature_orientation": {
              "name": "feature orientation",
              "type": "categorical",
              "categories": [
                "Rows (features in rows, samples in columns)",
                "Columns (samples in rows, features in columns)"
              ],
              "default-value": "Rows (features in rows, samples in columns)"
            }
          }
        }
      }
    },
    "data-preprocessing": {
      "filter-proteins": {
        "low-frequency-filter": {
          "name": "Filter by low frequency",
          "description": "Sample filter method:",
          "parameters": {
            "threshold": {
              "name": "Threshold for the relative amount of samples a protein is detected in:",
              "type": "numeric",
              "min": 0,
              "max": 1,
              "step": 0.1,
              "default-value": 0.5
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Bar chart",
                "Pie chart"
              ],
              "default-value": "Bar chart"
            }
          }
        }
      },
      "filter-samples": {
        "protein-count-filter": {
          "name": "Filter by protein count per sample",
          "description": "",
          "parameters": {
            "threshold": {
              "name": "Number of standard deviations from the median:",
              "type": "numeric",
              "min": 0,
              "max": 100,
              "step": 1.0,
              "default-value": 3
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Bar chart",
                "Pie chart"
              ],
              "default-value": "Pie chart"
            }
          }
        },
        "protein-intensity-sum-filter": {
          "name": "Filter by sum of protein intensities per sample",
          "description": "",
          "parameters": {
            "threshold": {
              "name": "Number of standard deviations from the median:",
              "type": "numeric",
              "min": 0,
              "max": 100,
              "step": 1.0,
              "default-value": 4
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Bar chart",
                "Pie chart"
              ],
              "default-value": "Pie chart"
            }
          }
        }
      },
      "outlier-detection": {
        "pca": {
          "name": "PCA (Principal component analysis)",
          "description": "",
          "parameters": {
            "threshold": {
              "name": "Threshold for number of standard deviations from the median:",
              "type": "numeric",
              "min": 1,
              "max": 5,
              "step": 1.0,
              "default-value": 2
            },
            "no_components": {
              "name": "Number of components:",
              "type": "numeric",
              "min": 2,
              "max": 3,
              "step": 1.0,
              "default-value": 3
            }
          }
        },
        "isolation-forest": {
          "name": "Isolation forest",
          "description": "",
          "parameters": {
            "no_estimators": {
              "name": "Number of estimators:",
              "type": "numeric",
              "min": 1,
              "max": 100,
              "step": 1.0,
              "default-value": 50
            }
          }
        },
        "local-outlier-factor": {
          "name": "Local outlier factor",
          "description": "",
          "parameters": {
            "no_neighbours": {
              "name": "Number of neighbours:",
              "type": "numeric",
              "min": 1,
              "max": 100,
              "step": 1.0,
              "default-value": 35
            }
          }
        }
      },
      "transformation": {
        "log-transformation": {
          "name": "Log transformation",
          "description": "",
          "parameters": {
            "log-base": {
              "name": "Log transformation base:",
              "type": "categorical",
              "categories": [
                "log2",
                "log10"
              ],
              "default-value": "log2"
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Boxplot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
                "Sample",
                "Protein ID"
              ],
              "default-value": "None"
            }
          }
        }
      },
      "normalisation": {
        "z-score": {
          "name": "Z-score normalisation",
          "description": "",
          "parameters": {
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Boxplot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
                "Sample",
                "Protein ID"
              ],
              "default-value": "None"
            }
          }
        },
        "totalsum": {
          "name": "Sumtotal normalisation",
          "description": "",
          "parameters": {
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Boxplot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
                "Sample",
                "Protein ID"
              ],
              "default-value": "None"
            }
          }
        },
        "median": {
          "name": "Median normalisation",
          "description": "",
          "parameters": {
            "percentile": {
              "name": "Percentile for normalisation:",
              "type": "numeric",
              "min": 0,
              "max": 1,
              "step": 0.05,
              "default-value": 0.5
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Boxplot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
                "Sample",
                "Protein ID"
              ],
              "default-value": "None"
            }
          }
        },
        "ref-protein": {
          "name": "Reference Protein Normalisation",
          "description": "",
          "parameters": {
            "reference-protein": {
              "name": "Reference Protein:",
              "type": "protein_dropdown",
              "default-value": ""
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Boxplot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
                "Sample",
                "Protein ID"
              ],
              "default-value": "None"
            }
          }
        }
      },
      "imputation": {
        "min-value-per-dataset": {
          "name": "Minimum value per dataset",
          "description": "",
          "parameters": {
            "shrinking-value": {
              "name": "Shrinking Value:",
              "type": "numeric",
              "min": 0,
              "max": 1,
              "step": 0.01,
              "default-value": 0.1
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
