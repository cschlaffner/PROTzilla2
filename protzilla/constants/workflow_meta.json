              ],
              "default-value": "None"
            },
            "graph-two-type": {
              "name": "Graph type - Imputed Values:",
              "type": "categorical",
              "categories": [
                "Bar chart",
                "Pie chart"
              ],
              "default-value": "Pie chart"
            }
          }
        },
        "simple-imputation-per-protein": {
          "name": "Mean/ median/ mode per protein",
          "description": "",
          "parameters": {
            "strategy": {
              "name": "Strategy:",
              "type": "categorical",
              "categories": [
                "mean",
                "median",
                "most frequent"
              ],
              "default-value": "median"
            },
            "graph-one-type": {
              "name": "Graph type:",
