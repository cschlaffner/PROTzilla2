              ],
              "default-value": "None"
            },
            "graph-two-type": {
              "name": "Graph type - Imputed Values:",
              "type": "categorical",
              "categories": [
                "Bar chart",
                "Pie chart"
              ],
              "default-value": "Pie chart"
            }
          }
        },
        "min-value-per-sample": {
          "name": "Minimum value per sample",
          "description": "",
          "parameters": {
            "shrinking-value": {
              "name": "Shrinking Value:",
              "type": "numeric",
              "min": 0,
              "max": 1,
              "step": 0.01,
              "default-value": 0.1
            },
            "graph-one-type": {
              "name": "Graph type:",
              "type": "categorical",
              "categories": [
                "Boxplot",
                "Histogram"
              ],
              "default-value": "Boxplot"
            },
            "graph-one-group-by": {
              "name": "Group by:",
              "type": "categorical",
              "categories": [
                "None",
