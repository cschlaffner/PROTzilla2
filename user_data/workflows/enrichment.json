{
  "analysis_level": "protein",
  "sections": {
    "importing": {
      "steps": [
        {
          "name": "ms_data_import",
          "method": "max_quant_import",
          "parameters": {
            "intensity_name": "iBAQ"
          }
        },
        {
          "name": "metadata_import",
          "method": "metadata_import_method",
          "parameters": {
            "feature_orientation": "Columns (samples in rows, features in columns)"
          }
        }
      ]
    },
    "data_preprocessing": {
      "steps": [
        {
          "name": "filter_proteins",
          "method": "samples_missing_filter",
          "parameters": {
            "percentage": 0.2
          },
          "graphs": [
            {
              "graph_type": "Pie chart"
            }
          ]
        },
        {
          "name": "filter_samples",
          "method": "protein_intensity_sum_filter",
          "parameters": {
            "threshold": 2
          },
          "graphs": [
            {
              "graph_type": "Bar chart"
            }
          ]
        },
        {
          "name": "imputation",
          "method": "knn",
          "parameters": {
            "number_of_neighbours": 10
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            },
            {
              "graph_type_quantites": "Pie chart"
            }
          ]
        },
        {
          "name": "outlier_detection",
          "method": "local_outlier_factor",
          "parameters": {
            "number_of_neighbors": 3
          }
        },
        {
          "name": "transformation",
          "method": "log_transformation",
          "parameters": {
            "log_base": "log2"
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            }
          ]
        },
        {
          "name": "normalisation",
          "method": "median",
          "parameters": {
            "percentile": 0.4
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            }
          ],
          "output_name": "preprocessed_data"
        }
      ]
    },
    "data_analysis": {
      "steps": [
        {
          "name": "differential_expression",
          "method": "t_test",
          "parameters": {
            "intensity_df": [
              "preprocessed_data",
              "dataframe"
            ],
            "grouping": "Group"
          }
        }
      ]
    },
    "data_integration": {
      "steps": [
        {
          "name": "enrichment_analysis",
          "method": "go_analysis_with_enrichr",
          "parameters": {
            "proteins": [
              "differential_expression",
              "de_proteins"
            ],
            "organism": "human"
          }
        },
        {
          "name": "plot",
          "method": "go_enrichment_bar_plot",
          "parameters": {
            "input_df": [
              "enrichment_analysis",
              "results"
            ],
            "top_terms": 10,
            "cutoff": 0.05
          }
        }
      ]
    }
  }
}
