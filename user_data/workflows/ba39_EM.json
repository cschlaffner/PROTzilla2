{
  "analysis_level": "protein",
  "sections": {
    "importing": {
      "steps": [
        {
          "name": "ms_data_import",
          "method": "max_quant_import",
          "parameters": {
            "intensity_name": "Intensity"
          },
          "output_name": null
        },
        {
          "name": "metadata_import",
          "method": "metadata_import_method",
          "parameters": {
            "feature_orientation": "Columns (samples in rows, features in columns)"
          },
          "output_name": null
        }
      ]
    },
    "data_preprocessing": {
      "steps": [
        {
          "name": "filter_proteins",
          "method": "samples_missing_filter",
          "parameters": {
            "percentage": 0.5
          },
          "graphs": [
            {
              "graph_type": "Pie chart"
            }
          ],
          "output_name": null
        },
        {
          "name": "filter_samples",
          "method": "protein_intensity_sum_filter",
          "parameters": {
            "threshold": 2
          },
          "graphs": [
            {
              "graph_type": "Bar chart"
            }
          ],
          "output_name": null
        },
        {
          "name": "transformation",
          "method": "log_transformation",
          "parameters": {
            "log_base": "log2"
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            }
          ],
          "output_name": null
        },
        {
          "name": "normalisation",
          "method": "z_score",
          "parameters": {},
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            }
          ],
          "output_name": null
        },
        {
          "name": "imputation",
          "method": "min_value_per_sample",
          "parameters": {
            "shrinking_value": 1
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "None",
              "graph_type_quantites": "Pie chart"
            }
          ],
          "output_name": "data_prep"
        }
      ]
    },
    "data_analysis": {
      "steps": [
        {
          "name": "model_selection",
          "method": "cluster_multiple_sample_sizes_and_k",
          "parameters": {
            "input_df": [
              "data_prep",
              "dataframe"
            ],
            "clustering_method": "Expectation Maximisation",
            "labels_column": "Group",
            "sample_sizes": [
              20,
              40,
              60,
              80,
              100,
              139
            ],
            "model_selection_scoring": "dunn_score",
            "scoring": [
              "davies_bouldin_score",
              "dunn_score",
              "calinski_harabasz_score"
            ],
            "n_clusters": [
              2,
              3,
              4,
              5,
              6,
              7,
              8,
              9,
              10
            ],
            "random_state": 6
          },
          "graphs": [
            {
              "plot_title": "BA39: EM Cluster Size vs. Sample Size"
            }
          ],
          "output_name": null
        },
        {
          "name": "model_selection",
          "method": "random_sampling",
          "parameters": {
            "input_df": [
              "data_prep",
              "dataframe"
            ],
            "labels_column": "Group",
            "n_samples": 40,
            "random_state": 6,
            "metadata_df": null
          },
          "output_name": "subset"
        },
        {
          "name": "clustering",
          "method": "expectation_maximisation",
          "parameters": {
            "input_df": [
              "random_sampling",
              "subset"
            ],
            "labels_column": "Group",
            "positive_label": "AD",
            "model_selection": "Manual",
            "scoring": [
              "adjusted_rand_score",
              "completeness_score",
              "fowlkes_mallows_score",
              "homogeneity_score",
              "mutual_info_score",
              "normalized_mutual_info_score",
              "rand_score",
              "v_measure_score",
              "davies_bouldin_score",
              "dunn_score",
              "silhouette_score",
              "calinski_harabasz_score"
            ],
            "n_components": 2,
            "reg_covar": 1e-06,
            "covariance_type": "full",
            "init_params": "kmeans",
            "max_iter": 100,
            "random_state": 6,
            "metadata_df": ""
          },
          "output_name": null
        }
      ]
    },
    "data_integration": {
      "steps": []
    }
  }
}