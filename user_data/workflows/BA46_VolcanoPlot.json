{
  "analysis_level": "protein",
  "sections": {
    "importing": {
      "steps": [
        {
          "name": "ms_data_import",
          "method": "max_quant_import",
          "parameters": {
            "intensity_name": "iBAQ"
          },
          "output_name": null
        },
        {
          "name": "metadata_import",
          "method": "metadata_import_method",
          "parameters": {
            "feature_orientation": "Columns (samples in rows, features in columns)"
          },
          "output_name": null
        }
      ]
    },
    "data_preprocessing": {
      "steps": [
        {
          "name": "filter_proteins",
          "method": "low_frequency_filter",
          "parameters": {
            "threshold": 0.2
          },
          "graphs": [
            {
              "graph_type": "Pie chart"
            }
          ],
          "output_name": null
        },
        {
          "name": "filter_samples",
          "method": "protein_intensity_sum_filter",
          "parameters": {
            "percentage": 0.8
          },
          "graphs": [
            {
              "graph_type": "Pie chart"
            }
          ],
          "output_name": null
        },
        {
          "name": "imputation",
          "method": "knn",
          "parameters": {
            "number_of_neighbours": 10
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            },
            {
              "graph_type_quantites": "Pie chart"
            }
          ],
          "output_name": null
        },
        {
          "name": "outlier_detection",
          "method": "local_outlier_factor",
          "parameters": {
            "number_of_neighbors": 3
          },
          "output_name": null
        },
        {
          "name": "transformation",
          "method": "log_transformation",
          "parameters": {
            "log_base": "log10"
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            }
          ],
          "output_name": null
        },
        {
          "name": "normalisation",
          "method": "median",
          "parameters": {
            "percentile": 0.4
          },
          "graphs": [
            {
              "graph_type": "Boxplot",
              "group_by": "Sample"
            }
          ],
          "output_name": "preprocessed_data"
        }
      ]
    },
    "data_analysis": {
      "steps": [
        {
          "name": "differential_expression",
          "method": "t_test",
          "parameters": {
            "intensity_df": [
              "preprocessed_data",
              "dataframe"
            ],
            "multiple_testing_correction_method": "Benjamini-Hochberg",
            "alpha": 0.05,
            "fc_threshold": 0,
            "grouping": "Group",
            "group1": "AD",
            "group2": "CTR",
            "metadata_df": ""
          },
          "output_name": "ttest"
        },
        {
          "name": "plot",
          "method": "volcano",
          "parameters": {
            "proteins_of_interest": [
              "P00738;P00738-2",
              "P02671;P02671-2;CON__P02672"
            ],
            "p_values": [
              "ttest",
              "corrected_p_values_df"
            ],
            "log2_fc": [
              "ttest",
              "log2_fold_change_df"
            ],
            "fc_threshold": [
              "ttest",
              "fc_threshold"
            ],
            "alpha": [
              "ttest",
              "alpha"
            ]
          }
        }
      ]
    },
    "data_integration": {
      "steps": []
    }
  }
}