{
  "analysis_level": "protein",
  "sections": {
    "importing": {
      "steps": [
        {
          "name": "ms_data_import",
          "method": "max_quant_import",
          "parameters": {
            "intensity_name": "iBAQ"
          }
        },
        {
          "name": "metadata_import",
          "method": "metadata_import_method",
          "parameters": {
            "feature_orientation": "Rows (features in rows, samples in columns)"
          }
        }
      ]
    },
    "data_preprocessing": {
      "steps": [
        {
          "name": "filter_proteins",
          "method": "samples_missing_filter",
          "parameters": {
            "percentage": 0.2,
            "graph_one_type": "Pie chart"
          }
        },
        {
          "name": "imputation",
          "method": "knn",
          "parameters": {
            "number_of_neighbours": 10,
            "graph_one_type": "Bar chart",
            "graph_one_group_by": "Sample",
            "graph_two_type": "Pie chart"
          }
        }
      ]
    },
    "data_analysis": {
      "steps": [
        {
          "name": "dimension_reduction",
          "method": "umap",
          "parameters": {
            "input_df": null,
            "n_neighbors": 15,
            "n_components": 2,
            "min_dist": 0.1,
            "metric": "euclidean",
            "random_state": 42
          }
        },
        {
          "name": "clustering",
          "method": "k_means",
          "parameters": {
            "input_df": null,
            "n_clusters": 8,
            "random_state": 6,
            "init_centroid_strategy": "random",
            "init_centroid_samples": "None",
            "n_init": 10,
            "max_iter": 300,
            "tolerance": 1e-4
          }
        },
        {
          "name": "plot",
          "method": "scatter_plot",
          "parameters": {
            "input_df": null,
            "color_df": null
          }
        }
      ]
    },
    "data_integration": {
      "steps": {}
    }
  }
}
